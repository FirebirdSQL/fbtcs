LD ld
LIBRARIES
SERVICE_MGR $(TCS:SERVICE_MGR)
LIB_TYPE share
FLAG_CC -I. -I./tests -I/opt/interbase/include -w -c -fwritable-strings %s -DIINUX
FLAG_CXX -w -c -fwritable-strings -I. -I./tests %s -DLINUX
FLAG_LINK %s -o %s -lgds -ldl -lcrypt
FLAG_LINK_MU %s -o %s -lgds -ldl -L./bin -lmu -lcrypt
