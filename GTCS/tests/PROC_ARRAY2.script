set term go;
create procedure proc_arr (city_in char(25))
RETURNS (cit char(25), stat char(5), populatio integer, rain_arra blob) as
begin

        SELECT city, state, population, rain_array
        FROM cities
        WHERE CITY = :city_in
        INTO :cit, :stat, :populatio, :rain_arra;

SUSPEND;
end
