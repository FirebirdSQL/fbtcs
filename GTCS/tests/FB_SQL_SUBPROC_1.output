create function f1 returns integer
as
	declare procedure sub1 returns (o1 integer)
	as
	begin
		o1 = 1;
		suspend;
		o1 = 2;
		suspend;
		o1 = 3;
		suspend;
	end

	declare o1 integer = 0;
	declare v integer;
begin
	for select o1 from sub1 into v do
		o1 = o1 + v;

	return o1;
end!

