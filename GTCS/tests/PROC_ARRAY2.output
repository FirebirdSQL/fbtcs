SQL> set term go;
SQL> create procedure proc_arr (city_in char(25))
CON> RETURNS (cit char(25), stat char(5), populatio integer, rain_arra blob) as
CON> begin
CON> 
CON>         SELECT city, state, population, rain_array
CON>         FROM cities
CON>         WHERE CITY = :city_in
CON>         INTO :cit, :stat, :populatio, :rain_arra;
CON> 
CON> SUSPEND;
CON> end
CON> go
SQL> set term ; go
SQL> commit;
