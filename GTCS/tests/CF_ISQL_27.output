SQL> create procedure rpl$generator_values
CON> returns (
CON>    gen_name varchar(31),
CON>    gen_value integer
CON> )
CON> as
CON> begin
CON>   for 
CON>     select rdb$generator_name
CON>     from rdb$generators 
CON>     where rdb$system_flag is null 
CON>     into :gen_name do
CON>   begin
CON>     execute statement 'select gen_id(' || gen_name || ',0) from rdb$database' into :gen_value;
CON>     suspend;
CON>   end
CON> END^
