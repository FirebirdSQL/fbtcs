CREATE DATABASE 'WHERE_GDB:cf_isql_27.fdb';

CREATE GENERATOR GEN_TEST1;
SET GENERATOR GEN_TEST1 TO 0;

CREATE GENERATOR GEN_TEST2;
SET GENERATOR GEN_TEST2 TO 0;

SET TERM ^ ;

create procedure rpl$generator_values
returns (
   gen_name varchar(31),
   gen_value integer
)
as
begin
  for 
    select rdb$generator_name
    from rdb$generators 
    where rdb$system_flag is null 
    into :gen_name do
  begin
    execute statement 'select cast(gen_id(' || gen_name || ',0) as int) from rdb$database' into :gen_value;
    suspend;
  end
END^

SET TERM ; ^
commit;
show procedures;
