CON> CREATE VIEW APROMET(DATUM, SO, VRSTA,IZNOS) AS
CON> 	select 
CON> 	rt.datumtroska,
CON> 	SIFRAPOSLOVNICE, 
CON> 	cast(vrt.nazivvrt as varchar(30)),
CON> 	cast(rt.iznos as numeric(18, 2))
CON> 	from rezijskitrosak rt 
CON> 	left join VRSTART vrt on rt.sifravrt = vrt.sifravrt
CON> 
CON> 	union all
CON> 
CON> 	SELECT 
CON> 	AL.DATUM, 
CON> 	SIFRAPOSLOVNICE, 
CON> 	cast('KancMat'as varchar(30)),
CON> 	cast(sum(alst.kolicina * alst.cena) as numeric(18, 2))
CON> 	FROM ALOKACIJAST ALST
CON> 	LEFT JOIN ALOKACIJA AL ON ALST.brdok=AL.brdok 
CON> 	LEFT JOIN ROBA R ON ALST.sifrarobe = R.sifrarobe
CON> 	WHERE R.vrstarobe = 'KM'
CON> 	GROUP BY AL.DATUM, SIFRAPOSLOVNICE
CON> 
CON> 	union all
CON> 
CON> 	SELECT 
CON> 	AL.DATUM,
CON> 	SIFRAPOSLOVNICE, 
CON> 	cast ('Hemikalije' as varchar(30)),
CON> 	cast(sum(alst.kolicina * alst.cena) as numeric(18, 2))
CON> 	FROM ALOKACIJAST ALST
CON> 	LEFT JOIN ALOKACIJA AL ON ALST.brdok=AL.brdok
CON> 	LEFT JOIN ROBA R ON ALST.sifrarobe = R.sifrarobe
CON> 	WHERE R.vrstarobe = 'HE'
CON> 	GROUP BY AL.DATUM, SIFRAPOSLOVNICE
CON> 
CON> 	union all
CON> 
CON> 	SELECT
CON> 	AL.DATUM,
CON> 	SIFRAPOSLOVNICE,
CON> 	cast('Prehrana' as varchar(30)),
CON> 	cast(sum(alst.kolicina * alst.cena) as numeric(18, 2))
CON> 	FROM ALOKACIJAST ALST
CON> 	LEFT JOIN ALOKACIJA AL ON ALST.brdok=AL.brdok
CON> 	LEFT JOIN ROBA R ON ALST.sifrarobe = R.sifrarobe
CON> 	WHERE R.vrstarobe = 'HR'
CON> 	GROUP BY AL.DATUM, SIFRAPOSLOVNICE
CON> 
CON> 	union all
CON> 
CON> 	SELECT
CON> 	pp.datum,
CON> 	SIFRAPOSLOVNICE, 
CON> 	cast('Popravke' as varchar(30)),
CON> 	cast(sum(iznos) as numeric(18,2))
CON> 	FROM popravka pp
CON> 	GROUP BY pp.DATUM, SIFRAPOSLOVNICE
CON> 
CON> 	union all
CON> 
CON> 	SELECT
CON> 	rg.datum,
CON> 	SIFRAPOSLOVNICE,
CON> 	cast('Registracije' as varchar(30)),
CON> 	cast(sum(cenatehnickog + cenaosiguranja+ostalitroskovi) as numeric(18,2))
CON> 	FROM registracija rg
CON> 	GROUP BY rg.DATUM, SIFRAPOSLOVNICE
CON> 
CON> 	union all
CON> 
CON> 	SELECT
CON> 	sg.datum,
CON> 	SIFRAPOSLOVNICE,
CON> 	cast('Gorivo' as varchar(30)), 
CON> 	cast(sum(kolicina * cena) as numeric(18,2))
CON> 	FROM sipanjegoriva sg
CON> 	GROUP BY
CON> 	sg.DATUM, SIFRAPOSLOVNICE
CON> 	;
